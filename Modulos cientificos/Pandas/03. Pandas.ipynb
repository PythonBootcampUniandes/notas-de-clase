   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "u.stack()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "u = g.unstack('character')\n",
    "total = u['Oscar the Grouch'] + u['Kermit the Frog']\n",
    "u['difference'] = u['Oscar the Grouch'] / total\n",
    "u.difference.plot(ylim=[0,1])"
   ]
